			<xs:element name="Standard" type="ProgrammeStandardType" maxOccurs="100"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TertiaryPerformanceResultType">
		<xs:sequence>
			<xs:element name="ObjectCategoryDescription" type="bc:DescriptionType">
				<xs:annotation>
					<xs:documentation>The catagory description of the message TEC want to communicate back</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ObjectIdentifiers">
				<xs:annotation>
					<xs:documentation>This element will contain the caller values passed to TEC and TEC want to make sure it communicate those values back in the context of the category and transaction codes.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ObjectIdentifier" type="bc:SimpleKeyValueType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="TransactionResultCode" type="bc:CodeType">
				<xs:annotation>
					<xs:documentation>The result code TEC uses to identify the specific message communicated back to the caller</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransactionResultDescription" type="bc:DescriptionType">
				<xs:annotation>
					<xs:documentation>The description associated to the result code mentioned above</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TertiaryPerformanceResultOperationParameterRequiredType">
		<xs:complexContent>
			<xs:extension base="TertiaryPerformanceResultType">
				<xs:sequence>
					<xs:element name="OperationParameters">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="OperationParameter" type="bc:SimpleKeyValueType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TertiaryPerformanceResultOperationParameterOptionalType">
		<xs:complexContent>
			<xs:extension base="TertiaryPerformanceResultType">
				<xs:sequence>
					<xs:element name="OperationParameters" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="OperationParameter" type="bc:SimpleKeyValueType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
