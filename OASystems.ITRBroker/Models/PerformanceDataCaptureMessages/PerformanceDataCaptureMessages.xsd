      </xs:element>
      <xs:element name="EnrolmentDeletionReasonCode" type="EnrolmentDeletionReasonCodeType" />
    </xs:sequence>
  </xs:complexType>
	<xs:complexType name="UpdateTrainingAgreementType">
		<xs:sequence>
			<xs:element name="LearnerDetails" type="tc:LearnerDetailsType"/>
			<xs:element name="TrainingAgreement" type="TrainingAgreementType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UpdateMATrainingPlanType">
		<xs:sequence>
			<xs:element name="LearnerDetails" type="tc:LearnerDetailsType"/>
			<xs:element name="MATrainingPlan">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="tc:LocalIdentifier"/>
						<xs:element name="TrainingPlanSignedDate" type="bc:DateType">
							<xs:annotation>
								<xs:documentation>Date the training plan was signed by the trainee, employer and MAC.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WithdrawEnrolmentType">
		<xs:annotation>
			<xs:documentation>Withdrawal from an enrolment prior to successful completion</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LearnerDetails" type="tc:LearnerDetailsType"/>
			<xs:element name="EnrolmentIdentifier" type="tc:EnrolmentIdentifierType"/>
			<xs:element name="WithdrawalDate" type="bc:DateType">
				<xs:annotation>
					<xs:documentation>Date the trainee ceased enrolment or funding ceased (training agreement)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WithdrawalReasonCode" type="WithdrawalReasonType">
				<xs:annotation>
					<xs:documentation>Identifies the reason why a trainee withdrew</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WithdrawTrainingAgreementType">
		<xs:annotation>
			<xs:documentation>Withdrawal from a training agreement prior to successful completion</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LearnerDetails" type="tc:LearnerDetailsType"/>
			<xs:element name="TrainingAgreementLocalIdentifier" type="bc:IdentifierType">
				<xs:annotation>
					<xs:documentation>A number issued by the ITO that identifies a training agreement.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WithdrawalDate" type="bc:DateType">
				<xs:annotation>
					<xs:documentation>Date the training agreement ceased</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WithdrawalReasonCode" type="WithdrawalReasonType">
				<xs:annotation>
					<xs:documentation>Identifies the reason why a trainee withdrew</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
  <xs:complexType name="CeasingProgrammeIdentifierType">
    <xs:sequence>
      <xs:element ref="tc:LocalIdentifier">
        <xs:annotation>
          <xs:documentation>An optional element for the programme number issued by the ITO a trainee was previously enrolled in for the scenario where the previous ITO has merged with your ITO or had ceased operations.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProviderNumber" type="bc:NumberType">
        <xs:annotation>
          <xs:documentation>The provider number of the previous ITO.  Also known as EDUMIS number. When used as part of a response from TEC, may contain 'NOT FOUND' if an error occurs with loading the specified provider number.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
